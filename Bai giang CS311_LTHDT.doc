<div id="wrapper">	<div id="wrapper_inside">		<h1 class="elearning">Elearning - ﻿Đại học Duy Tân</h1>		<h1 class="logo"><a href="http://duytan.edu.vn"; title>Logo ﻿Đại học Duy Tân</a></h1>		<h1 class="login_1">Login</h1>  <form name="abcde" method="post" action="http://elearning.dtu.edu.vn/access/login" enctype="application/x-www-form-urlencoded" width="100%">		<div id="choiceone">				<input name="radType" type="radio" value="1" id="radType" checked/>				<label for="radiobutton">Elearning</label>				<input name="radType" type="radio" value="1" id="radType" />				<label for="radiobutton">Email</label>		</div> 		<div class="form_wr">			<div id="div1">				<label class="textlabel">Username</label>				<span class="texttype1 texttype_active">				<input class="focus" name="eid" type="text" value=""/> </span></div>			<div id="div2">				<label class="textlabel">Password</label>				<span class="texttype2"><input type="password" name="pw" value=""/> </span></div>			<div id="div1">				<span class="remember">				<input type="checkbox" name="remember" value="1" />				<label>Nhớ mật khẩu</label> </span><span class="forgotpass">				<a href="/portal">Quên mật khẩu?</a>				</span><span class="login_button">				<input type="submit" onclick="return changeData()" value="" /></span></div>				<input type="hidden" name="task" value="">				<input type="hidden" name="data" value="">		</div>		</form>		<div id="copyright">			© 2010 ﻿Đại học Duy Tân</div>	</div></div><script> function changeData(){	if(document.abcde.radType[1].checked){		document.abcde.task.value='email';		document.abcde.data.value=document.abcde.eid.value;		document.abcde.action="http://myduytan.dtu.edu.vn/GetData.aspx";	}}</script>
</body></html>
